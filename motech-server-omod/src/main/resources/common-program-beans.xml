<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="nopCommand" class="${corePackage}.event.MessagesCommand" />
	<bean id="messageScheduler" class="${corePackage}.messaging.impl.MessageSchedulerImpl">
		<property name="registrarBean">
			<bean parent="registrarBean" />
		</property>
	</bean>
	<bean id="userPrefmessageScheduler" class="${corePackage}.messaging.impl.MessageSchedulerImpl">
		<property name="userPreferenceBased" value="true" />
		<property name="registrarBean">
			<bean parent="registrarBean" />
		</property>
	</bean>
	<bean id="timeBean" class="${corePackage}.time.TimeBean">
		<property name="registrarBean">
			<bean parent="registrarBean" />
		</property>
	</bean>

</beans>