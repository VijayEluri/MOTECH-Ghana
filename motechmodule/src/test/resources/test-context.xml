<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="org.springframework.remoting.jaxws.SimpleJaxWsServiceExporter">
		<property name="baseAddress" value="http://localhost:8880/openmrs/ws/" />
	</bean>

	<bean id="registrarService"
		class="org.openmrs.module.motechmodule.web.ws.RegistrarWebService" />

	<bean id="registrarClient"
		class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
		<property name="serviceInterface"
			value="org.openmrs.module.motechmodule.web.ws.RegistrarService" />
		<property name="wsdlDocumentUrl"
			value="http://localhost:8880/openmrs/ws/RegistrarService?wsdl" />
		<property name="namespaceUri"
			value="http://ws.web.motechmodule.module.openmrs.org/" />
		<property name="serviceName" value="RegistrarWebServiceService" />
		<property name="portName" value="RegistrarWebServicePort" />
		<property name="lookupServiceOnStartup" value="false" />
	</bean>

</beans>