<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="${corePackage}.model">
	<class name="MessageProgramEnrollment" table="motechmodule_enrollment">
		<id name="id" type="long" column="motechmodule_enrollment_id">
			<generator class="native" />
		</id>
		<properties name="uniquePersonInProgram" unique="true">
			<property name="personId" type="int" column="person_id"
				not-null="true" />
			<property name="program" type="string" column="program_name"
				not-null="true" length="200" />
		</properties>
		<property name="startDate" type="java.util.Date" column="start_date"
			length="19" />
		<property name="endDate" type="java.util.Date" column="end_date"
			length="19" />
	</class>
</hibernate-mapping>