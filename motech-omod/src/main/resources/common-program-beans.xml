<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="nopCommand" class="org.motech.event.impl.NopCommand" />
	<bean id="messageScheduler" class="org.motech.messaging.MessageSchedulerImpl">
		<property name="registrarBean">
			<bean parent="registrarBean" />
		</property>
	</bean>
	<bean id="userPrefmessageScheduler" class="org.motech.messaging.MessageSchedulerImpl">
		<property name="userPreferenceBased" value="true" />
		<property name="registrarBean">
			<bean parent="registrarBean" />
		</property>
	</bean>

</beans>