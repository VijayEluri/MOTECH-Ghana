<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="${corePackage}.model">
	<class name="Log" table="motechmodule_log">
		<id name="id" type="int" column="motechmodule_log_id">
			<generator class="native" />
		</id>
		<property name="type"
			type="${corePackage}.model.db.hibernate.LogTypeEnumType" column="log_type"
			not-null="true" length="7" />
		<property name="date" type="java.util.Date" column="log_datetime"
			not-null="true" length="19" />
		<property name="message" type="java.lang.String" column="log_message"
			not-null="true" length="255" />
	</class>
</hibernate-mapping>