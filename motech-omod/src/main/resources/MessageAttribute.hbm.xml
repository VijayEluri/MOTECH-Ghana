<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="${corePackage}.messaging">
	<class name="MessageAttribute" table="motechmodule_messageattribute">
		<id name="id" type="long" column="motechmodule_messageattribute_id">
			<generator class="native" />
		</id>
		<property name="name" type="string" column="attribute_name"
			not-null="true" unique="true" length="255" />
		<property name="property" type="string" column="attribute_property"
			not-null="true" length="255" />
	</class>
</hibernate-mapping>