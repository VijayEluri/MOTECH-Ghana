<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.motech.model.Nurse">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" />
		<many-to-one name="clinic" />
		<property name="phoneNumber" update="true" />
		<list name="pregnancies" cascade="save-update,merge">
			<key column="nurse_id" not-null="true" />
			<index />
			<one-to-many class="org.motech.model.Pregnancy" />
		</list>
		<list name="maternalVisits" cascade="save-update,merge">
			<key column="nurse_id" not-null="true" />
			<index />
			<one-to-many class="org.motech.model.MaternalVisit" />
		</list>
		<list name="futureServices" cascade="save-update,merge">
			<key column="nurse_id" not-null="true" />
			<index />
			<one-to-many class="org.motech.model.FutureServiceDelivery" />
		</list>
	</class>
</hibernate-mapping>