<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.motech.model.MaternalData">
		<id name="id">
			<generator class="native" />
		</id>
		<one-to-one name="patient" class="org.motech.model.Patient" />
		<list name="pregnancies" cascade="save-update,merge">
			<key column="maternaldata_id" not-null="true" />
			<index />
			<one-to-many class="org.motech.model.Pregnancy" />
		</list>
		<list name="maternalVisits" cascade="save-update,merge">
			<key column="maternaldata_id" not-null="true" />
			<index />
			<one-to-many class="org.motech.model.MaternalVisit" />
		</list>
	</class>
</hibernate-mapping>