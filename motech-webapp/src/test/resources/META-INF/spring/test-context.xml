<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="registrarClient"
		class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
		<property name="serviceInterface" value="org.motech.ws.RegistrarWS" />
		<property name="wsdlDocumentUrl"
			value="http://localhost:8080/motech/ws/RegistrarService?wsdl" />
		<property name="namespaceUri" value="http://motech.org/" />
		<property name="serviceName" value="RegistrarService" />
		<property name="portName" value="RegistrarPort" />
	</bean>

</beans>