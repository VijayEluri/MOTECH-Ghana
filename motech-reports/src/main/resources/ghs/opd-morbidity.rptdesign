            <property name="queryText">select
CONCAT('General',CAST(ge1.id AS CHAR)) as id,
ge1.date,
loc1.location_id,
ge1.sex,
ge1.birthdate,
ge1.diagnosis,
ge1.referred,
ge1.newcase,
1 as diag_num
from motechmodule_generaloutpatientencounter ge1
join motechmodule_facility fa1 on fa1.facility_id = ge1.facility_id
join location loc1 on loc1.location_id = fa1.location_id
union all
select 
CONCAT('General',CAST(ge2.id AS CHAR)) as id,
ge2.date,
loc2.location_id,
ge2.sex,
ge2.birthdate,
ge2.secondary_diagnosis as diagnosis,
ge2.referred,
ge2.newcase,
2 as diag_num
from motechmodule_generaloutpatientencounter ge2
join motechmodule_facility fa2 on fa2.facility_id = ge2.facility_id
join location loc2 on loc2.location_id = fa2.location_id
where ge2.secondary_diagnosis is not null
union all
select
  CONCAT('MotherChild',CAST(enc.encounter_id AS CHAR)) as id,
  enc.encounter_datetime as date,
  enc.location_id,
  IF(STRCMP(pers.gender,'M'),IF(STRCMP(pers.gender,'F'),'UNKNOWN','FEMALE'),'MALE') as sex,
  pers.birthdate,
  obs1.value_numeric as diagnosis,
  obs2.value_numeric as referral,
  obs3.value_numeric as newcase,
  IF(STRCMP(cn1.name,'PRIMARY DIAGNOSIS'),IF(STRCMP(cn1.name,'SECONDARY DIAGNOSIS'),0,2),1) as diag_num
from encounter enc
join encounter_type enctyp on enc.encounter_type = enctyp.encounter_type_id and enctyp.name = 'OUTPATIENTVISIT'
join person pers on enc.patient_id = pers.person_id and pers.voided = 0
left outer join (obs obs1 join concept_name cn1 on obs1.concept_id = cn1.concept_id
and (cn1.name = 'PRIMARY DIAGNOSIS' or cn1.name = 'SECONDARY DIAGNOSIS') and obs1.voided = 0)
on enc.encounter_id = obs1.encounter_id
left outer join (obs obs2 join concept_name cn2 on obs2.concept_id = cn2.concept_id
and cn2.name = 'NEW CASE'  and obs2.voided = 0) on enc.encounter_id = obs2.encounter_id
left outer join (obs obs3 join concept_name cn3 on obs3.concept_id = cn3.concept_id 
and cn3.name = 'REFERRED' and obs3.voided = 0) on enc.encounter_id = obs3.encounter_id
where enc.voided = 0;</property>
