		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="last_obs" />
		<property name="command">
			<bean class="org.motech.event.impl.ScheduleMessageCommand">
				<property name="messageKey" value="tetanus.2.reminder.1" />
				<property name="messageScheduler" ref="messageScheduler" />
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedNumImpl">
					<property name="expectedNumber" value="2" />
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2Reminder1" />
					<property name="nextState" ref="tetanusState3" />
					<property name="registrarBean" ref="registrarBean" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2Reminder1" />
					<property name="nextState" ref="tetanusState2Reminder2" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2Reminder1" />
					<property name="nextState" ref="tetanusState2Reminder1" />
				</bean>
			</list>
		</property>
	</bean>
	
	<bean id="tetanusState2Reminder2" class="org.motech.event.impl.MessageProgramStateImpl">
		<property name="name" value="Tetanus Reminder State 2-2" />
		<property name="timeValue" value="6" />
		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="last_obs" />
		<property name="command">
			<bean class="org.motech.event.impl.ScheduleMessageCommand">
				<property name="messageKey" value="tetanus.2.reminder.2" />
				<property name="messageScheduler" ref="messageScheduler" />
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedNumImpl">
					<property name="expectedNumber" value="2" />
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2Reminder2" />
					<property name="nextState" ref="tetanusState3" />
					<property name="registrarBean" ref="registrarBean" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2Reminder2" />
					<property name="nextState" ref="tetanusState3" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2Reminder2" />
					<property name="nextState" ref="tetanusState2Reminder2" />
				</bean>
			</list>
		</property>
	</bean>
	
	<bean id="tetanusState3" class="org.motech.event.impl.MessageProgramStateImpl">
		<property name="name" value="Tetanus End State 3" />
		<property name="command">
			<bean class="org.motech.event.impl.CompositeCommand">
				<property name="commands">
					<list>
						<bean class="org.motech.event.impl.RemoveMessagesCommand">
							<property name="registrarBean" ref="registrarBean" />
						</bean>
						<bean class="org.motech.event.impl.RemoveEnrollmentCommand">
							<property name="registrarBean" ref="registrarBean" />
						</bean>
					</list>
				</property>
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState3" />
					<property name="nextState" ref="tetanusState3" />
				</bean>
			</list>
		</property>
	</bean>

