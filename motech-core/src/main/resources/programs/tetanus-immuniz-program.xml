<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="tetanusImmunization" class="org.motech.event.impl.MessageProgramImpl">
		<property name="name" value="Tetanus Immunization Message Program" />
		<property name="conceptName" value="IMMUNIZATIONS ORDERED" />
		<property name="conceptValue" value="TETANUS BOOSTER" />
		<property name="startState" ref="tetanusState1" />
		<property name="endState" ref="tetanusState3" />
	</bean>

	<bean id="tetanusState1" class="org.motech.event.impl.MessageProgramStateImpl">
		<property name="name" value="Tetanus Wait State 1" />
		<property name="timeValue" value="3" />
		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="patient_registration" />
		<property name="command">
			<bean class="org.motech.event.impl.ScheduleMessageCommand">
				<property name="messageKey" value="tetanus.1.prompt" />
				<property name="messageScheduler" ref="messageScheduler" />
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedNumImpl">
					<property name="expectedNumber" value="1" />
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1" />
					<property name="nextState" ref="tetanusState2" />
					<property name="registrarBean" ref="registrarBean" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1" />
					<property name="nextState" ref="tetanusState1Reminder1" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1" />
					<property name="nextState" ref="tetanusState1" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="tetanusState1Reminder1" class="org.motech.event.impl.MessageProgramStateImpl">
		<property name="name" value="Tetanus Reminder State 1-1" />
		<property name="timeValue" value="6" />
		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="patient_registration" />
		<property name="command">
			<bean class="org.motech.event.impl.ScheduleMessageCommand">
				<property name="messageKey" value="tetanus.1.reminder.1" />
				<property name="messageScheduler" ref="messageScheduler" />
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedNumImpl">
					<property name="expectedNumber" value="1" />
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1Reminder1" />
					<property name="nextState" ref="tetanusState2" />
					<property name="registrarBean" ref="registrarBean" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1Reminder1" />
					<property name="nextState" ref="tetanusState1Reminder2" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1Reminder1" />
					<property name="nextState" ref="tetanusState1Reminder1" />
				</bean>
			</list>
		</property>
	</bean>
	
		<bean id="tetanusState1Reminder2" class="org.motech.event.impl.MessageProgramStateImpl">
		<property name="name" value="Tetanus Reminder State 1-2" />
		<property name="timeValue" value="9" />
		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="patient_registration" />
		<property name="command">
			<bean class="org.motech.event.impl.ScheduleMessageCommand">
				<property name="messageKey" value="tetanus.1.reminder.2" />
				<property name="messageScheduler" ref="messageScheduler" />
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedNumImpl">
					<property name="expectedNumber" value="1" />
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1Reminder2" />
					<property name="nextState" ref="tetanusState2" />
					<property name="registrarBean" ref="registrarBean" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1Reminder2" />
					<property name="nextState" ref="tetanusState3" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState1Reminder2" />
					<property name="nextState" ref="tetanusState1Reminder2" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="tetanusState2" class="org.motech.event.impl.MessageProgramStateImpl">
		<property name="name" value="Tetanus Wait State 2" />
		<property name="timeValue" value="1" />
		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="last_obs" />
		<property name="command">
			<bean class="org.motech.event.impl.ScheduleMessageCommand">
				<property name="messageKey" value="tetanus.2.prompt" />
				<property name="messageScheduler" ref="messageScheduler" />
			</bean>
		</property>
		<property name="program" ref="tetanusImmunization" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedNumImpl">
					<property name="expectedNumber" value="2" />
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2" />
					<property name="nextState" ref="tetanusState3" />
					<property name="registrarBean" ref="registrarBean" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2" />
					<property name="nextState" ref="tetanusState2Reminder1" />
				</bean>
				<bean class="org.motech.event.impl.MessageProgramStateTransitionImpl">
					<property name="command" ref="nopCommand" />
					<property name="prevState" ref="tetanusState2" />
					<property name="nextState" ref="tetanusState2" />
				</bean>
			</list>
		</property>
	</bean>

