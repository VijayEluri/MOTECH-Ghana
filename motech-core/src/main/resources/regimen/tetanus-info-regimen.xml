					<property name="nextState" ref="tetanusInfoState3" />
				</bean>
			</list>
		</property>
	</bean>
	
	<bean id="tetanusInfoState4" class="org.motech.event.impl.RegimenStateImpl">
		<property name="name" value="Tetanus Info State 4" />
		<property name="timeValue" value="6" />
		<property name="timePeriod" value="minute" />
		<property name="timeReference" value="patient_registration" />
