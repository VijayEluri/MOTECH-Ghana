		</property>
		<property name="regimen" ref="tetanusInfo" />
		<property name="patientObsService" ref="patientObsService" />
		<property name="transitions">
			<list>
				<bean class="org.motech.event.impl.RegimenStateTransitionExpectedDateImpl">
					<property name="command" ref="nopCommand" />
