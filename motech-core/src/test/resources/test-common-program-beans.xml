<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="nopCommand" class="org.motech.event.MessagesCommand" />
	<bean id="registrarBean" class="org.easymock.EasyMock" factory-method="createMock">
		<constructor-arg index="0" value="org.motech.svc.RegistrarBean" />
	</bean>
	<bean id="messageScheduler" class="org.easymock.EasyMock" factory-method="createMock">
		<constructor-arg index="0" value="org.motech.messaging.MessageScheduler" />
	</bean>
	<bean id="timeBean" class="org.motech.time.TimeBean">
		<property name="registrarBean" ref="registrarBean" />
	</bean>
	
</beans>