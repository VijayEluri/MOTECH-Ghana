<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="pregnancyANCService" class="org.motech.service.impl.ServiceDeliverySequenceEncImpl">
		<property name="name" value="ANC" />
		<property name="encounterTypeName" value="ANCVISIT" />
		<property name="timeBean" ref="timeBean" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="requirements">
			<list>
				<bean class="org.motech.service.impl.ActivePregnancyRequirement">
					<property name="registrarBean" ref="registrarBean" />
				</bean>
			</list>
		</property>
	</bean>

</beans>	
