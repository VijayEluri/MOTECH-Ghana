<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="pregnancyANCSchedule" class="org.motechproject.server.service.impl.ExpectedANCEncounterSchedule">
		<property name="name" value="ANC" />
		<property name="encounterTypeName" value="ANCVISIT" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="requirements">
			<list>
				<bean class="org.motechproject.server.service.impl.AliveRequirement" />
			</list>
		</property>
	</bean>

</beans>	
