<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="childIPTiService" class="org.motech.service.impl.ServiceDeliverySequenceObsImpl">
		<property name="name" value="IPTI" />
		<property name="serviceConceptName" value="IMMUNIZATIONS ORDERED" />
		<property name="serviceValueConceptName" value="INTERMITTENT PREVENTATIVE TREATMENT INFANTS" />
		<property name="timeBean" ref="timeBean" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="requirements">
			<list>
				<bean class="org.motech.service.impl.AgeRequirement">
					<property name="maxTimeValue" value="5" />
					<property name="maxTimePeriod" value="year" />
					<property name="timeBean" ref="timeBean" />
				</bean>
			</list>
		</property>
		<property name="services">
			<list>
				<bean class="org.motech.service.impl.ServiceDeliveryImpl">
					<property name="name" value="IPTI1" />
					<property name="startTimeValue" value="10" />
					<property name="startTimePeriod" value="week" />
					<property name="startTimeReference" value="patient_birthdate" />
					<property name="endTimeValue" value="11" />
					<property name="endTimePeriod" value="week" />
					<property name="endTimeReference" value="patient_birthdate" />					
				</bean>
				<bean class="org.motech.service.impl.ServiceDeliveryImpl">
					<property name="name" value="IPTI2" />
					<property name="earliestTimeValue" value="14" />
					<property name="earliestTimePeriod" value="week" />
					<property name="earliestTimeReference" value="patient_birthdate" />
					<property name="startTimeValue" value="4" />
					<property name="startTimePeriod" value="week" />
					<property name="startTimeReference" value="last_obs_date" />
					<property name="endTimeValue" value="5" />
					<property name="endTimePeriod" value="week" />
					<property name="endTimeReference" value="last_obs_date" />					
				</bean>
				<bean class="org.motech.service.impl.ServiceDeliveryImpl">
					<property name="name" value="IPTI3" />
					<property name="earliestTimeValue" value="9" />
					<property name="earliestTimePeriod" value="month" />
					<property name="earliestTimeReference" value="patient_birthdate" />
					<property name="startTimeValue" value="4" />
					<property name="startTimePeriod" value="week" />
					<property name="startTimeReference" value="last_obs_date" />
					<property name="endTimeValue" value="5" />
					<property name="endTimePeriod" value="week" />
					<property name="endTimeReference" value="last_obs_date" />					
				</bean>
			</list>
		</property>
	</bean>

</beans>	
