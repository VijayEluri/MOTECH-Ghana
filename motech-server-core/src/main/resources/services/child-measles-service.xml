<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="childMeaslesService" class="org.motechproject.server.service.impl.ServiceDeliverySequenceObsImpl">
		<property name="name" value="MEASLES" />
		<property name="serviceConceptName" value="IMMUNIZATIONS ORDERED" />
		<property name="serviceValueConceptName" value="MEASLES VACCINATION" />
		<property name="timeBean" ref="timeBean" />
		<property name="registrarBean" ref="registrarBean" />
		<property name="requirements">
			<list>
				<bean class="org.motechproject.server.service.impl.AgeRequirement">
					<property name="maxTimeValue" value="5" />
					<property name="maxTimePeriod" value="year" />
					<property name="timeBean" ref="timeBean" />
				</bean>
			</list>
		</property>
		<property name="services">
			<list>
				<bean class="org.motechproject.server.service.impl.ServiceDeliveryImpl">
					<property name="name" value="MEASLES1" />
					<property name="startTimeValue" value="36" />
					<property name="startTimePeriod" value="week" />
					<property name="startTimeReference" value="patient_birthdate" />
					<property name="endTimeValue" value="40" />
					<property name="endTimePeriod" value="week" />
					<property name="endTimeReference" value="patient_birthdate" />					
				</bean>
			</list>
		</property>
	</bean>

</beans>	
