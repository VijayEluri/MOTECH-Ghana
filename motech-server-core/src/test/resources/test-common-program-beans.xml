<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="nopCommand" class="org.motechproject.server.event.MessagesCommand" />
	<bean id="registrarBean" class="org.easymock.EasyMock" factory-method="createMock">
		<constructor-arg index="0" value="org.motechproject.server.svc.RegistrarBean" />
	</bean>
	<bean id="userPrefmessageScheduler" class="org.motechproject.server.messaging.impl.MessageSchedulerImpl">
		<property name="registrarBean" ref="registrarBean" />
		<property name="userPreferenceBased" value="true" />
	</bean>
	<bean id="timeBean" class="org.motechproject.server.time.TimeBean">
		<property name="registrarBean" ref="registrarBean" />
	</bean>
	
</beans>